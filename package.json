{
  "name": "credo",
  "version": "0.1.0",
  "description": "ReasonML full-stack repo for service presentation",
  "repository": "git@github.com:br1anchen/credo.git",
  "private": false,
  "scripts": {
    "dev": "run-p watch-world serve-app",
    "build": "run-s clean-world make-world build-app",
    "make-world": "bsb -make-world",
    "watch-world": "bsb -make-world -w",
    "clean-world": "bsb -clean-world",
    "serve-app": "parcel src/client/index.html",
    "build-app": "parcel build src/client/index.html"
  },
  "keywords": [
    "ReasonML"
  ],
  "author": "Brian Chen <brianchen8990@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "bs-platform": "^5.0.3",
    "bsb-js": "^1.1.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3"
  }
}
